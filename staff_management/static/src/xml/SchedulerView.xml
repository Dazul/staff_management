<template>
<t t-name="SchedulerView">
    <table cellspacing="0" cellpadding="0" border="0" width="100%" class="oe_calendar_table">
        <tr>
            <td width="100%">
            	<div class="staff_management_quick_assign_container oe_form">
            	
            		<table class="oe_form_group" style="margin: 0;">
            			<tr>
            				<td class="staff_quickassign_button"><input id="staff_quickassign" type="checkbox" /><label for="staff_quickassign">Quick assign</label></td>
            				<td class="staff_quickassign_text staff_quickassign_form hidden">Task</td>
            				<td class="staff_management_quick_assign_task oe_form_group_cell staff_quickassign_form hidden"></td>
            				<td class="staff_quickassign_text staff_quickassign_form hidden">Start hour</td>
            				<td class="staff_management_quick_assign_hour_start oe_form_group_cell staff_quickassign_form hidden"></td>
            				<td class="staff_quickassign_text staff_quickassign_form hidden">End hour</td>
            				<td class="staff_management_quick_assign_hour_stop oe_form_group_cell staff_quickassign_form hidden"></td>
            			</tr>
            		</table>
            	
            	</div>
                <div class="oe_calendar dhx_cal_container">
                    <div class="dhx_cal_navline staff_mngmt_special_nav">
                        <div class="dhx_cal_prev_button">Week</div>
                        <div class="dhx_cal_next_button">Week</div>
                        <div class="dhx_cal_today_button" t-opentag="true"></div>
                        <div class="dhx_cal_prev_month_button" t-opentag="true">Month</div>
                        <div class="dhx_cal_next_month_button" t-opentag="true">Month</div>
                        <div class="dhx_cal_date" t-opentag="true"></div>
                        <div class="dhx_cal_tab" name="matrix_tab" style="right:76px;" t-opentag="true">Week</div>
                    </div>
                    <div class="dhx_cal_header">
                    </div>
                    <div class="staff_weekcal_headband">
                    </div>                    
                    <div class="dhx_cal_data">
                    </div>
                </div>
            </td>
        </tr>
    </table>
    <div class="staff_tooltip"></div>
</t>
